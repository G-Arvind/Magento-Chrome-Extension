{
  "name" : "Magento Chrome Extension",
  "version" : "0.1",
  "description" : "Magento Chrome Extension",
  "content_scripts": [ {
    "matches": ["<all_urls>"],
    "js": [
      "jquery-3.5.1.min.js", 
      "popup.js"
    ]
  } ],
  "permissions": [
    "activeTab",
    "storage"
  ],
  "browser_action": {
     "default_icon": "icon.png",
     "default_popup": "popup.html"
  },
  "manifest_version": 2
}
